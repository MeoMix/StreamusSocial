{
  "name": "jspm-marionette-boilerplate",
  "version": "0.0.1",
  "private": true,
  "description": "A demo project highlighting usage of JSPM w/ Backbone.Marionette",
  "license": "MIT",
  "author": "Sean Anderson",
  "repository": {
    "type": "git",
    "url": "https://github.com/MeoMix/jspm-marionette-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/MeoMix/jspm-marionette-boilerplate/issues"
  },
  "homepage": "https://github.com/MeoMix/jspm-marionette-boilerplate",
  "scripts": {
    "test": "gulp test",
    "postinstall": "jspm install"
  },
  "jspm": {
    "directories": {
      "packages": "jspm/jspm_packages"
    },
    "configFile": "jspm/jspm.config.js",
    "dependencies": {
      "backbone": "npm:backbone@^1.2.3",
      "backbone.babysitter": "github:marionettejs/backbone.babysitter@^0.1.10",
      "backbone.wreqr": "github:marionettejs/backbone.wreqr@^1.3.5",
      "handlebars": "github:components/handlebars.js@^4.0.5",
      "hbs": "github:davis/plugin-hbs@^1.2.1",
      "jquery": "github:components/jquery@^2.1.4",
      "jspm-loader-css": "github:MeoMix/jspm-loader-css@^1.0.2",
      "lodash": "npm:lodash@^3.10.1",
      "marionette": "github:marionettejs/backbone.marionette@^2.4.3",
      "postcss-modules-values": "npm:postcss-modules-values@^1.1.1",
      "text": "github:systemjs/plugin-text@^0.0.4"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.22",
      "babel-runtime": "npm:babel-runtime@^5.8.20",
      "core-js": "npm:core-js@^1.1.0"
    },
    "overrides": {
      "github:marionettejs/backbone.marionette@2.4.3": {
        "map": {
          "underscore": "lodash"
        }
      },
      "github:marionettejs/backbone.wreqr@1.3.5": {
        "map": {
          "underscore": "lodash"
        }
      },
      "github:marionettejs/backbone.babysitter@0.1.10": {
        "map": {
          "underscore": "lodash"
        }
      },
      "npm:backbone@1.2.3": {
        "map": {
          "underscore": "lodash"
        }
      },
      "github:MeoMix/jspm-loader-css@1.0.2": {
        "registry": "npm"
      }
    }
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "connect-pushstate": "^1.0.0",
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "gulp-cached": "^1.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-connect": "https://github.com/MeoMix/gulp-connect/tarball/e863448805c3e4fba38a3ff5bb4f6500502547aa",
    "gulp-eslint": "^1.0.0",
    "gulp-filter": "^3.0.1",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-plumber": "^1.0.1",
    "gulp-useref": "^3.0.4",
    "gulp-util": "^3.0.6",
    "karma": "^0.13.10",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jspm": "^2.0.1",
    "karma-mocha": "^0.2.0",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "opn": "^3.0.2",
    "path": "^0.12.7",
    "phantomjs": "^1.9.18",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4",
